{
  "name": "goo-poem",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bash ./src/scripts/start.sh",
    "dev:client": "vite",
    "dev:server": "nodemon --watch 'src/server/**/*' -e ts --exec tsx ./src/server/index.ts",
    "dev2:server": "nodemon --watch 'src/data.md' -e ts --exec tsx ./src/scripts/build-log.ts",
    "goo": "npm run dev --prefix ./src/goo/",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "add": "tsx ./src/scripts/add-line.ts"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.14.202",
    "cors": "^2.8.5",
    "kill-port": "^2.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "dependencies": {
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "npx": "^10.2.2"
  }
}
